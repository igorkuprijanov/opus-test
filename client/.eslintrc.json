{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb",
    "airbnb-typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "tsconfig.json",
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "overrides": [
    {
      "files": [
        "*.tsx"
      ],
      "rules": {
        "react/destructuring-assignment": 0,
        "react/no-access-state-in-setstate": 0,
        "consistent-return": 0
      }
    }
  ],
  "plugins": [
    "import",
    "react",
    "@typescript-eslint"
  ]
}
